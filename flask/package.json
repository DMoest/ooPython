    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "clean-css": "^3.4.12",
    "cross-env": "^7.0.3",
    "csslint": "^0.10.0",
    "eslint": "^4.17.0",
    "eslint-plugin-react": "^7.6.1",
    "html-minifier": "^0.8.0",
    "htmlhint": "0.9.12",
    "js-yaml": "^3.6.1",
    "jscs": "^3.0.7",
    "jshint": "^2.9.5",
    "jsonlint": "^1.6.2",
    "stylelint": "^8.4.0",
    "uglify-js": "^2.6.2"
  }
