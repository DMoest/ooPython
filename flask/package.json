{
  "name": "flask",
  "version": "1.0.0",
  "description": "Flask application for Yatzy game.",
  "main": "app.py",
  "scripts": {
    "test": "python3 test.py",
    "dev": "cross-env TAILWIND_MODE=watch NODE_ENV=development postcss ./static/styles/tailwind.css -o ./static/dist/style.css -w",
